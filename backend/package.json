{
  "name": "juridica-digital-lpdp-backend",
  "version": "1.0.0",
  "description": "Backend completo para sistema LPDP - APIs Partners, Exportación y Webhooks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "echo 'No build required for backend'",
    "setup-db": "node scripts/setupDatabase.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "deploy": "npm run lint && npm run test && echo 'Ready for deployment'"
  },
  "keywords": [
    "lpdp",
    "data-protection",
    "chile",
    "compliance",
    "api",
    "partners",
    "export",
    "pdf",
    "excel",
    "supabase"
  ],
  "author": {
    "name": "Jurídica Digital SpA",
    "email": "admin@juridicadigital.cl",
    "url": "https://juridica-digital.cl"
  },
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.0",
    "pdfkit": "^0.14.0",
    "exceljs": "^4.4.0",
    "crypto": "^1.0.1",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "sharp": "^0.33.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdoc": "^4.0.2",
    "@types/jest": "^29.5.8",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alex3000silvac-ai/capac_03_LPDP.git"
  },
  "bugs": {
    "url": "https://github.com/alex3000silvac-ai/capac_03_LPDP/issues"
  },
  "homepage": "https://scldp-frontend.onrender.com",
  "config": {
    "port": 3001
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/docs/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "nodemonConfig": {
    "watch": [
      "services/",
      "routes/",
      "server.js"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "coverage/",
      "logs/"
    ]
  }
}